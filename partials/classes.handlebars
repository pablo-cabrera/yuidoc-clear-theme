<h4>Class {{moduleName}}</h4>
{{#if uses}}
    Uses:
    {{#each uses}}
        <a href="{{this}}.html">{{this}}</a>
    {{/each}}
    <br>
{{/if}}
{{#if extension_for}}
    Extension For:
    {{#each extension_for}}
        <a href="{{this}}.html">{{this}}</a>
    {{/each}}
    <br>
{{/if}}
{{#if extends}}
    Extends: {{#crossLink extends}}{{/crossLink}}<br>
{{/if}}
<span class="foundat">Class defined in: <a href="{{foundAt}}">`{{file}}:{{line}}`</a></span>
<div class="intro">{{{classDescription}}}</div>

{{#if is_constructor}}
    {{#is_constructor}}
    {{>method}}
    {{/is_constructor}}
{{/if}}

<div id="classdocs">
    <h2>Index</h2>

    <ul>
        {{#if methods}}
        <li><a href="#methods">Methods</a></li>
        {{/if}}
        {{#if properties}}
        <li><a href="#props">Properties</a></li>
        {{/if}}
        {{#if attrs}}
        <li><a href="#attrs">Attributes</a></li>
        {{/if}}
        {{#if events}}
        <li><a href="#events">Events</a></li>
        {{/if}}
    </ul>
    <div>
        {{#if methods}}
            <div id="methods">
            <h2>Methods</h2>
            {{#methods}}
                {{>method}}
            {{/methods}}
            </div>
        {{/if}}
        {{#if properties}}
            <div id="props">
            <h2>Properties</h2>
            {{#properties}}
                {{>props}}
            {{/properties}}
            </div>
        {{/if}}
        {{#if attrs}}
            <div id="attrs">
            <h2>Attributes</h2>
            {{#attrs}}
                {{>attrs}}
            {{/attrs}}
            </div>
        {{/if}}
        {{#if events}}
            <div id="events">
            <h2>Events</h2>
            {{#events}}
                {{>events}}
            {{/events}}
            </div>
        {{/if}}
    </div>
</div>
